domain Machine [Machine]
domain ControlledDomain [Causal]
interface "Machine-ControlledDomain" {
    shared: {
        event control [Machine -> ControlledDomain]
        event feedback [ControlledDomain -> Machine]
    }
}
requirement "RequiredBehavior" {
    frame: RequiredBehavior
    constrains: ControlledDomain
    reference: Machine
}
